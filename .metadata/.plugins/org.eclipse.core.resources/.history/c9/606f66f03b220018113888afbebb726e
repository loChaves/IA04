public class Operation {
	String operation;
	double arg1, arg2;
	
	public Operation(String _op, double _arg1, double _arg2){
		operation = _op;
		arg1 = _arg1;
		arg2 = _arg2;
	}
	
	public Operation(){
		operation = "";
		arg1 = 0;
		arg2 = 0;
	}
	
	public String getOperation(){
		return operation;
	}
	
	public double getArg1(){
		return arg1;
	}
	
	public double getArg2(){
		return arg2;
	}
}
